package service

import (
	"github.com/boshnyakovich/news-aggregator/internal/repository"
	"github.com/boshnyakovich/news-aggregator/pkg/logger"
)

type Service struct {
	repo *repository.Repo
	log  *logger.Logger
}

func NewService(repo *repository.Repo, log *logger.Logger) *Service {
	return &Service{
		repo: repo,
		log:  log,
	}
}
